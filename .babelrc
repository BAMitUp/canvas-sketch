{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    ["transform-class-properties"],
    ["babel-root-import", {"rootPathSuffix": "src"}],
    ["transform-object-rest-spread", {"useBuiltIns": true}]
  ]
}
